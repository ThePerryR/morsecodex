  "author": "perry@teachok.com",
  "private": true,
  "main": "src",
  "license": "MIT",
  "scripts": {
    "start": "node .",
    "lint": "eslint src",
    "test-components": "cd src/components && cross-env NODE_ENV=test jest",
    "test-server": "cross-env NODE_ENV=test jest",
    "coverage": "npm run test-server --coverage && npm run test-components --coverage",
    "test": "npm run test-server && npm run test-components && npm run lint",
    "dev": "concurrently --kill-others \"nodemon src --watch\" \"node webpack/webpack-dev-server\"",
    "storybook": "start-storybook -p 6006 -s src/public -c src/stories",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles -u 1 src/public/* src/public/**/* src/public/**/**/* dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config",
    "heroku-postbuild": "npm run build"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/src/components"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "testEnvironment": "node"
  },
